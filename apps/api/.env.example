# CAU Sistema Eleitoral - API
# Variaveis de ambiente para producao (AWS Secrets Manager)

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5436
DATABASE_NAME=cau_eleitoral
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres

# JWT
JWT_KEY=YourSuperSecretKeyHereThatIsAtLeast256BitsLong123456
JWT_ISSUER=CAU.Eleitoral
JWT_AUDIENCE=CAU.Eleitoral.Client
JWT_EXPIRATION_MINUTES=60
JWT_REFRESH_EXPIRATION_DAYS=7

# AWS
AWS_REGION=us-east-1
AWS_S3_BUCKET_DOCUMENTS=cau-eleitoral-documents
AWS_S3_BUCKET_UPLOADS=cau-eleitoral-uploads

# Email (SMTP)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=noreply@cau.org.br
EMAIL_FROM_NAME=CAU Sistema Eleitoral

# Redis (optional)
REDIS_CONNECTION=localhost:6380

# Sentry (optional)
SENTRY_DSN=

# Environment
ASPNETCORE_ENVIRONMENT=Development
